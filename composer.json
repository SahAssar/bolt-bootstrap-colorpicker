{
    "name": "boltabandoned/bootstrapcolorpicker",
    "description": "Another colorpicker extension. Based on http://mjolnic.com/bootstrap-colorpicker/ and Raistlfiren/FAIconFieldType",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": "^3.0.0"
    },
    "keywords": [
      "field",
      "colorpicker",
      "backend",
      "color"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Smithee",
            "email": "alan.smithee@example.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "boltabandoned\\bootstrapcolorpicker\\": "src"
        }
    },
    "extra": {
        "bolt-class": "boltabandoned\\bootstrapcolorpicker\\BootstrapColorPickerFieldTypeExtension",
        "bolt-assets": "web",
        "bolt-icon": "web/colorpicker-icon.png"
    }
}
